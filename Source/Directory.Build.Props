<Project>
	<PropertyGroup>
		<UO3DRoot>$(MSBuildThisFileDirectory)\..\</UO3DRoot>
		<IntermediatePath>$(UO3DRoot)\Intermediate</IntermediatePath>
		<OutputPath>$(UO3DRoot)\Binaries\Win64\$(Configuration)\</OutputPath>
		<BaseIntermediateOutputPath>$(IntermediatePath)\DotNet\$(Configuration)\$(MSBuildProjectName)</BaseIntermediateOutputPath>
		<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
		<AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>
		<UO3DThirdParty>$(UO3DRoot)\ThirdParty\</UO3DThirdParty>
	</PropertyGroup>

	<PropertyGroup>
		<Platforms>x64</Platforms>
		<PlatformTarget>x64</PlatformTarget>
		<Platform>x64</Platform>
	</PropertyGroup>
	
	<PropertyGroup>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<GenerateTargetFrameworkAttribute>false</GenerateTargetFrameworkAttribute>
	</PropertyGroup>

</Project>
